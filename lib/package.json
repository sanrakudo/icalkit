{
  "name": "icalkit",
  "version": "0.0.1",
  "description": "iCalendar file management - CLI tools and Node.js API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "icalkit": "./bin/icalkit.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'CLI lint - coming soon'"
  },
  "keywords": [
    "icalendar",
    "ics",
    "calendar",
    "cli",
    "api",
    "library",
    "split",
    "merge",
    "google-calendar"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3"
  }
}
