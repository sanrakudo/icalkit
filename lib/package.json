{
  "name": "icalkit",
  "version": "0.3.0",
  "description": "iCalendar file management - CLI tools and Node.js API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "icalkit": "./bin/icalkit.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "icalendar",
    "ics",
    "calendar",
    "google-calendar"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanrakudo/icalkit.git",
    "directory": "lib"
  },
  "bugs": {
    "url": "https://github.com/sanrakudo/icalkit/issues"
  },
  "homepage": "https://github.com/sanrakudo/icalkit#readme",
  "author": {
    "name": "tasshi",
    "email": "tasshi.me@gmail.com",
    "url": "https://github.com/tasshi-me"
  },
  "license": "MIT",
  "engines": {
    "node": ">=24.12.0"
  },
  "dependencies": {
    "ical.js": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@vitest/ui": "^4.0.16",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16"
  }
}
